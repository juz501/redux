{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
	"rules": {
    "comma-dangle": "warn",
		"eol-last": "warn",
	  "linebreak-style": "warn",
    "no-trailing-spaces": "warn",
		"no-unused-vars": "warn",
		"import/extensions": "warn",
    "import/no-mutable-exports": "warn",
		"import/no-unresolved": "warn",
    "jsx-a11y/no-static-element-interactions": "off",
    "react/jsx-filename-extension": "off"
	},
  "globals": {
    "window": true
  }
}